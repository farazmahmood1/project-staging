  container.prepend(element);\r\n            await delay(slideanimation);\r\n            click = true;\r\n            slide = slides\r\n            clearTimeout(timerId);\r\n            timerId = setTimeout(() => {\r\n                flag = true\r\n                run();\r\n            }, 2000);\r\n        }\r\n    }\r\n\r\n    async function next() {\r\n        if (click) {\r\n            click = false;\r\n            flag = false;\r\n\r\n            var element2 = document.getElementById('container').children[1];\r\n            element2.classList.add(\"main\");\r\n            var container = document.getElementById('container');\r\n            var element = document.getElementById('container').children[0];\r\n            var slides = document.getElementById('container').children[0].dataset.slide;\r\n            if (parseInt(slides) + 1 == parseInt(numofslides) + 1)\r\n                changebackground(1);\r\n            else\r\n                changebackground(parseInt(slides) + 1);\r\n            element.classList.add(\"slide-active\");\r\n            element.classList.remove(\"main\");\r\n            await delay(slideanimation);\r\n            element.remove();\r\n            element.classList.remove(\"slide-active\");\r\n            container.append(element);\r\n            click = true;\r\n            slide = slides\r\n            clearTimeout(timerId);\r\n            timerId = setTimeout(() => {\r\n                flag = true\r\n                run();\r\n            }, 2000);\r\n        }\r\n    }\r\n\r\n    async function run() {\r\n        while (flag) {\r\n            await delay(slidedelay);\r\n            if (flag) {\r\n                click = false;\r\n                slide++;\r\n                if (slide > numofslides) {\r\n                    slide = 1;\r\n\r\n                }\r\n\r\n                for (var i = 0; i < numofslides; i++) {\r\n                    var element = document.getElementById('container').children[i];\r\n                    element.classList.remove(\"main\");\r\n                }\r\n                var container = document.getElementById('container');\r\n                var element = document.getElementById('container').children[0];\r\n                element.classList.add(\"slide-active\");\r\n                var element2 = document.getElementById('container').children[1];\r\n                element2.classList.add(\"main\");\r\n                changebackground(slide);\r\n                await delay(slideanimation);\r\n                element.remove();\r\n                element.classList.remove(\"slide-active\");\r\n                container.append(element);\r\n                click = true;\r\n            }\r\n        }\r\n    }\r\n\r\n    function changebackground(slide) {\r\n        var container = document.getElementById('container');\r\n        if (slide == 1) {\r\n            container.style.backgroundImage = \"url('https:\/\/staging.3li.global\/wp-content\/uploads\/2023\/03\/retail.png')\";\r\n        }\r\n        else if (slide == 2) {\r\n            container.style.backgroundImage = \"url('https:\/\/staging.3li.global\/wp-content\/uploads\/2023\/03\/communication.png')\";\r\n        }\r\n        else if (slide == 3) {\r\n            container.style.backgroundImage = \"url('https:\/\/staging.3li.global\/wp-content\/uploads\/2023\/03\/public-sector-1.png')\";\r\n        }\r\n        else if (slide == 4) {\r\n            container.style.backgroundImage = \"url('https:\/\/staging.3li.global\/wp-content\/uploads\/2023\/03\/construction.png')\";\r\n        }\r\n        else if (slide == 5) {\r\n            container.style.backgroundImage = \"url('https:\/\/staging.3li.global\/wp-content\/uploads\/2023\/03\/education-1.png')\";\r\n        }\r\n        else if (slide == 6) {\r\n            container.style.backgroundImage = \"url('https:\/\/staging.3li.global\/wp-content\/uploads\/2023\/03\/banking-1.png')\";\r\n        }\r\n    }\r\n\r\n    function Mhover(num, flg) {\r\n        var cslide;\r\n        if (click) {\r\n            flag = true;\r\n            if (flg) {\r\n                flag = false;\r\n                changebackground(num);\r\n                for (var i = 0; i < numofslides; i++) {\r\n                    var element = document.getElementById('container').children[i];\r\n                    if (document.getElementById('container').children[i].dataset.slide == num) {\r\n                        cslide = i;\r\n                    }\r\n                    element.classList.remove(\"main\");\r\n                }\r\n                var element = document.getElementById('container').children[cslide];\r\n                element.classList.add(\"main\");\r\n            }\r\n            else {\r\n                for (var i = 0; i < numofslides; i++) {\r\n                    var element = document.getElementById('container').children[i];\r\n                    if (document.getElementById('container').children[i].dataset.slide == num) {\r\n                        cslide = i;\r\n                    }\r\n                    element.classList.remove(\"main\");\r\n                }\r\n                var element = document.getElementById('container').children[0];\r\n                var slides = document.getElementById('container').children[0].dataset.slide;\r\n                element.classList.add(\"main\");\r\n                changebackground(slides);\r\n                slide = slides;\r\n                clearTimeout(timerId);\r\n                timerId = setTimeout(() => {\r\n                    run(slides);\r\n                }, 2000);\r\n            }\r\n        }\r\n    }\r\n\r\n    changebackground(1);\r\n    var element2 = document.getElementById('container').children[0];\r\n    element2.classList.add(\"main\");\r\n    run();\r\n<\/script>\r\n<\/body>\r\n\r\n\r\n<\/html>\t\t<\/div>\n\t\t\t\t<\/div>\n\t\t\t\t\t<\/div>\n\t\t<\/div>\n\t\t\t\t\t\t\t<\/div>\n\t\t<\/section>\n\t\t\t\t<section data-particle_enable=\"false\" data-particle-mobile-disab